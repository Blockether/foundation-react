{
    "mcpServers": {
        "search-engine": {
            "type": "http",
            "url": "https://api.z.ai/api/mcp/web_search_prime/mcp",
            "headers": {
                "Authorization": "Bearer ${Z_AI_API_KEY}"
            }
        },
        "chrome-devtools": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "chrome-devtools-mcp@latest"
            ],
            "env": {}
        },
        "code_analysis": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "cclsp@latest"
            ],
            "env": {
                "CCLSP_CONFIG_PATH": ".claude/cclsp.json"
            }
        },
        "agno": {
            "type": "http",
            "url": "https://docs.agno.com/mcp"
        },
        "shadcn": {
            "command": "npx",
            "args": [
                "shadcn@latest",
                "mcp"
            ]
        },
        "image-analysis": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@z_ai/mcp-server"
            ],
            "env": {
                "Z_AI_API_KEY": "${Z_AI_API_KEY}",
                "Z_AI_MODE": "ZAI"
            }
        }
    }
}