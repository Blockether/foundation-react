{
  "name": "@blockether/foundation-react",
  "version": "1.4.0",
  "description": "React-based chat UI, complement to `blockether_foundation` library with integrated SQL analysis capabilities",
  "type": "module",
  "module": "dist/foundation-react.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build --mode lib",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "pnpm build && storybook build",
    "prepublishOnly": "pnpm build && pnpm type-check",
    "release": "pnpm publish --access public",
    "release:patch": "npm version patch && npm run release",
    "release:minor": "npm version minor && npm run release",
    "release:major": "npm version major && npm run release"
  },
  "keywords": [
    "react",
    "typescript",
    "agent-gui-copilot",
    "reporting"
  ],
  "author": "blockether.com <contact@blockether.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.30.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "apache-arrow": "^21.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "recharts": "^3.3.0",
    "sonner": "^2.0.7",
    "sql-formatter": "^15.6.10",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/react": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@tailwindcss/vite": "^4.1.17",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "prettier": "^3.6.2",
    "storybook": "^10.0.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/foundation-react.js"
    },
    "./styles": "./dist/foundation-react.css"
  }
}
