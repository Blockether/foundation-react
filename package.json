{
  "name": "@blockether/foundation-react",
  "version": "1.0.0",
  "description": "React-based chat UI foundation with integrated SQL analysis capabilities",
  "type": "module",
  "main": "dist/foundation-react.cjs.js",
  "module": "dist/foundation-react.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "build:lib": "vite build --mode lib",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "react",
    "typescript",
    "agent-gui-copilot",
    "reporting"
  ],
  "author": "blockether.com <contact@blockether.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "peerDependencies": {
    "react": ">18.0.0",
    "react-dom": ">18.0.0"
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.28.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "sql-formatter": "^15.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.0.4",
    "@storybook/react": "^10.0.5",
    "@storybook/react-vite": "^10.0.4",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "prettier": "^3.2.5",
    "storybook": "^10.0.4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.7.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/foundation-react.es.js",
      "require": "./dist/foundation-react.cjs.js"
    }
  }
}