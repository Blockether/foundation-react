{
  "name": "@blockether/foundation-react",
  "version": "2.0.5",
  "description": "Browser-based SQL cockpit and AI chat UI components for React. Powered by DuckDB-WASM and shadcn/ui. Designed for agno and blockether_foundation Python libraries.",
  "type": "module",
  "module": "dist/foundation-react.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build --mode lib",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -Rf node_modules dist pnpm-lock.yaml package-lock.json",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "storybook": "storybook dev -p 6006",
    "build:examples:standalone": "pnpm build && cd examples/standalone && pnpm install && pnpm build",
    "build-storybook": "pnpm build && storybook build",
    "prepublishOnly": "pnpm build && pnpm type-check",
    "release": "pnpm publish --access public",
    "release:patch": "npm version patch && npm run release",
    "release:minor": "npm version minor && npm run release",
    "release:major": "npm version major && npm run release"
  },
  "keywords": [
    "react",
    "sql",
    "duckdb",
    "ai-chat",
    "shadcn-ui"
  ],
  "author": "blockether.com <contact@blockether.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0",
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "dependencies": {
    "@duckdb/duckdb-wasm": "^1.30.0",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@xyflow/react": "^12.9.2",
    "ai": "^5.0.92",
    "apache-arrow": "^21.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "^0.462.0",
    "marked-react": "^3.0.2",
    "monaco-editor": "^0.54.0",
    "motion": "^12.23.24",
    "nanoid": "^5.1.6",
    "shiki": "^3.15.0",
    "sonner": "^2.0.7",
    "sql-formatter": "^15.6.10",
    "streamdown": "^1.4.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tokenlens": "^1.3.1",
    "use-stick-to-bottom": "^1.1.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@ai-sdk/react": "^2.0.92",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@tailwindcss/vite": "^4.1.17",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "postcss": "^8.5.6",
    "postcss-prefix-selector": "^2.1.1",
    "prettier": "^3.6.2",
    "storybook": "^10.0.7",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.9.1"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/foundation-react.js"
    },
    "./styles": "./dist/foundation-react.css"
  }
}